specVersion: v0.1.0
package:
  name: "ethereum_fluid"
  version: "v0.1.0"

protobuf:
  files:
    - tycho/evm/v1/vm.proto
    - tycho/evm/v1/common.proto
    - tycho/evm/v1/utils.proto
  importPaths:
    - ../../proto

binaries:
  default:
    type: wasm/rust-v1
    file: ../target/wasm32-unknown-unknown/release/ethereum_fluid.wasm

modules:
- name: map_dex_deployed
    type: map
    proto_path: "./src/modules/map_dex_deployed.proto"
    functions:
      - function: "map_dex_deployed"
        params:
          factory_address: "0x91716C4EDA1Fb55e84Bf8b4c7085f84285c19085"
          protocol_type_name: "FluidDex"
  
  - name: store_dexes
    type: store
    proto_path: "./src/modules/store_dexes.proto"
    functions:
      - function: "store_dexes"
  
  - name: map_dex_components
    type: map
    proto_path: "./src/modules/map_dex_components.proto"
    functions:
      - function: "map_dex_components"
  
  - name: store_dex_components
    type: store
    proto_path: "./src/modules/store_dex_components.proto"
    functions:
      - function: "store_dex_components"